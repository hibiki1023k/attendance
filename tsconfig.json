{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    // {
    //   "compilerOptions": {
    //     "baseUrl": ".",
    //     "paths": {
    //       "@/styles/*": ["styles/*"],
    //       "@/components/ui/*": ["components/ui/*"]
    //     }
    //   }
    // }
  ],
  "exclude": [
    "node_modules"
  ]
}
